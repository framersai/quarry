{
  "name": "@framersai/quarry",
  "version": "0.1.0",
  "private": true,
  "description": "Quarry - Notes That Organize Themselves. Free & Open Source Personal Knowledge Management.",
  "author": "Johnny Dunn <johnnyfived2@protonmail.com>",
  "license": "MIT",
  "main": "electron-dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/framersai/quarry"
  },
  "keywords": [
    "notes",
    "pkm",
    "personal-knowledge-management",
    "markdown",
    "knowledge-base",
    "digital-garden",
    "open-source"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "cross-env STATIC_EXPORT=true next build",
    "build:web": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "postinstall": "node scripts/copy-sqljs-wasm.js",
    "electron:dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && pnpm electron:start\"",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:prebuild": "pnpm build && pnpm electron:build",
    "electron:pack": "pnpm electron:prebuild && electron-builder --dir",
    "electron:dist": "pnpm electron:prebuild && electron-builder",
    "electron:dist:mac": "pnpm electron:prebuild && electron-builder --mac",
    "electron:dist:mac:unsigned": "pnpm electron:prebuild && electron-builder --mac -c.mac.identity=null",
    "electron:dist:win": "pnpm electron:prebuild && electron-builder --win",
    "electron:dist:linux": "pnpm electron:prebuild && electron-builder --linux"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
